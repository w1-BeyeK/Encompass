  <MasterDetailPage.Detail>
        <NavigationPage BarBackgroundColor="#F9D492">
      <x:Arguments>
        <pages:CardsPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>